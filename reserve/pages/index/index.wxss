.home-container {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%; 
  background: #f1f1f1;
}

.carousel {
  width: 100%;
  height: 320rpx;
}

.carousel-img {
  width: 100%;
  height: 100%;
}

.nav {
  display: flex;
  margin-top: 20rpx;
  width: 100%;
  position: relative;
  background: #fff;
  height: 154rpx;
  overflow: hidden;
}

.nav::before {
  content: '';
  position: absolute;
  width: 10rpx;
  height: 100%;
  background: #eee;
  left: 50%;
  margin-left: -5rpx;
}

.nav-item {
  flex: 1;
  display: flex;
}

.nav-item>view {
  flex: 1;
}

.nav-img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-top: 27rpx;
  margin-right: 22rpx;
}

.nav-content {
  padding-top: 30rpx;
}

.nav-content>text {
  margin-left: 15rpx;
  display: block;
  font-size: 34rpx;
}

.seller-list {
  position:relative;
  width: 100%;
  height: auto;
  background: #fff;
  margin-top: 20rpx;
}

.title {
  display: block;
  line-height: 72rpx;
  font-size: 32rpx;
  border-bottom: 1px solid #f1f1f1;
  margin: 0 30rpx 20rpx;
}

.seller-item {
  display: flex;
  flex-direction: row;
  margin: 0 30rpx 30rpx;
}

.img-box {
  flex: 0 0 200rpx;
  width: 200rpx;
  height: 160rpx;
  background: #f1f1f1;
}

.seller-img {
  width: 100%;
  height: 100%;
}

.seller-detail {
  position: relative;
  margin-left: 20rpx;
  width: 100%;
}

.title-box {
  line-height: 28rpx;
}

.seller-title {
  font-size: 28rpx;
}

.distance {
  float: right;
  font-size: 22rpx;
  color: #666;
}

.star-box {
  flex-direction: row;
  line-height: 20rpx;
  display: inline-block;
}

.star-item {
  width: 20rpx;
  height: 20rpx;
}

.seller-state {
  display: inline-block;
  line-height: 24rpx;
  margin-left: 20rpx;
}

.state {
  font-size: 24rpx;
  color: #666;
}

.give {
  font-size: 25rpx;
  color: #e84540;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
   width:95%; 
}

.seller-time {
  position: absolute;
  bottom: 3rpx;
  left: 0;
  line-height: 22rpx;
}

.time {
  font-size: 22rpx;
  color: #e84540;
}

.fix-n {
  width: 90rpx;
  height: 90rpx;
  background: #ffb637;
  border-radius: 50%;
  transition: all 0.3s;
}

.fix-n>image {
  width: 90rpx;
  height: 90rpx;
}

.fix-nav {
  position: fixed;
  bottom: 130rpx;
  right: 30rpx;
  z-index: 10;
}

.fix-nav-x {
  position: absolute;
  top: 0;
  left: 0;
  transform: translate3d(-96rpx, -106rpx, 0);
}

.fix-nav-y {
  position: absolute;
  top: 0;
  left: 0;
  transform: translate3d(-96rpx, 106rpx, 0);
}

.fix-nav-z {
  position: absolute;
  top: 0;
  left: 0;
  transform: translate3d(-140rpx, 0, 0);
}

.fix-static {
  transform: translate3d(0, 0, 0);
}

.fix-nav-add {
  position: absolute;
  top: 0;
  left: 0;
  transform: translate3d(0, 0, 0);
}

.fix-nav-add::before {
  content: '';
  position: absolute;
  width: 3rpx;
  height: 40rpx;
  top: 50%;
  left: 50%;
  background: #fff;
  margin-top: -20rpx;
}

.fix-nav-add::after {
  content: '';
  position: absolute;
  width: 40rpx;
  height: 3rpx;
  top: 50%;
  left: 50%;
  background: #fff;
  margin-left: -20rpx;
}

.seller-title-view {
  display: flex;
  height: 72rpx;
  line-height: 72rpx;
  border-bottom: 1px solid #f1f1f1;
  margin: 0 20rpx 20rpx; 
  z-index:100;
}

.seller-title-view-toTop{
  position:fixed;
  top:0;
  left:-20rpx;
  width:100%; 
  background:#fff;
  }

.seller-title-view>view {
  position: relative;
  font-size: 28rpx;
  flex: 1;
  text-align: center;
  overflow: hidden;
}

.seller-title-view .active {
  color: red;
}

/* 搜索 */

.search-box {
  /* position: absolute; 
  top: 0; 
  left: 0;  */
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 80rpx;
  z-index: 100;
  transition: background .2s linear;
  -webkit-transition: background .2s linear;
  background: #fff;
  border-bottom: none;
}

.area {
  position: relativel;
  flex: 0 0 150rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  color: #333;
}

.triangle {
  position: absolute;
  top: 37rpx;
  left: 115rpx;
  width: 0;
  height: 0;
  border-width: 10rpx 10rpx 0;
  border-style: solid;
  border-color: #666 transparent transparent;
}

.search {
  position: relative;
  width: 100%;
  padding-right: 60rpx;
}

.search input {
  margin-top: 15rpx;
  height: 40rpx;
  font-size: 22rpx;
  border-radius: 30rpx;
  padding-left: 70rpx;
  border:1px solid #eee;
  background:#eee;
}

.input-empty {
  background: #fff;
  color: #999;
}

.search-icon {
  position: absolute;
  top: 28rpx;
  left: 30rpx;
}

.triangle-first, .triangle-second {
  position: absolute;
  top: 34rpx;
  left: 245rpx;
  display: block;
  border-color: #666 transparent transparent;
}
.triangle-second{
  /* left: 217rpx; */
}

.empty-shop{
  height:480rpx;
}
.back{
  position:fixed;
  bottom:40rpx;
  left:50%;
  width:150rpx;
  display:inline-block;
  margin-left:-75rpx;
  padding:10rpx 20rpx;
  box-sizing: border-box;
  font-size:26rpx;
  color:#999;
  border:1px solid #ccc;
  border-radius:4px;
}